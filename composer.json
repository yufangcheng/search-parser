{
  "name": "yfc/search-parser",
  "description": "A laravel plugin that allows query via a GET request",
  "homepage": "https://github.com/yufangcheng/search-parser",
  "keywords": [
    "Laravel",
    "Search",
    "restful",
    "url"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "laravel/framework": "~5.3",
    "avris/bag": "~3.0",
    "doctrine/dbal": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Inno\\Lib\\SearchParser\\": "SearchParser/",
      "Inno\\Lib\\Tests\\": "Tests/"
    },
    "files": [
      "SearchParser/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Inno\\Lib\\Test\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "secure-http": false
  }
}