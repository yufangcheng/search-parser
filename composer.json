{
  "name": "inno/lib-php",
  "description": "Inno PHP Library",
  "homepage": "http://git.patsnap.com/inno/lib-php",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "http://192.168.3.60:8088/repo/private/"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    {
      "packagist": false
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": "~5.3",
    "avris/bag": "~3.0",
    "doctrine/dbal": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "SearchParser\\": "SearchParser/",
      "Tests\\": "Tests/"
    },
    "files": [
      "SearchParser/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Inno\\Lib\\Test\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "secure-http": false
  }
}